{"kind": "Pod", "spec": {"hostNetwork": true, "containers": [{"livenessProbe": {"initialDelaySeconds": 15, "httpGet": {"path": "/healthz", "host": "${MASTER_IP}", "port": ${MASTER_PORT}}, "timeoutSeconds": 15}, "command": ["/hyperkube", "apiserver", "--address=0.0.0.0", "--port=${MASTER_PORT}", "--etcd_servers=http://${MASTER_IP}:4001", "--service-cluster-ip-range=${SERVICE_CLUSTER_IP_RANGE}", "--token-auth-file=/dev/null", "--insecure-bind-address=0.0.0.0", "--advertise-address=${MASTER_IP}", "--cloud-provider=openstack", "--cloud-config=/etc/kubernetes/cloud-config/openstack.cfg", "--v=4"], "name": "kube-apiserver", "image": "${HYPERKUBE_IMAGE}", "volumeMounts": [{"readOnly": true, "mountPath": "/etc/kubernetes/cloud-config", "name": "cloudconfig"}], "ports": [{"hostPort": 443, "containerPort": 443, "name": "https"}, {"hostPort": ${MASTER_PORT}, "containerPort": ${MASTER_PORT}, "name": "local"}]}], "volumes": [{"hostPath": {"path": "/etc/kubernetes/cloud-config"}, "name": "cloudconfig"}]}, "apiVersion": "v1", "metadata": {"namespace": "kube-system", "name": "kube-apiserver"}}
